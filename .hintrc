{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-required-parent": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "off",
      {
        "ignore": [
          "@-ms-viewport",
          "@-ms-viewport"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not and_ff <= 113",
    "not chrome <= 114",
    "not edge <= 114",
    "not firefox <= 113",
    "not ios_saf <= 16.5",
    "not opera <= 99",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}